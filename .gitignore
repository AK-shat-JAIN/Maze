# Ignore all compiled Ruby files
*.rbc

# Ignore all test files generated by Capybara
capybara-*.html

# Ignore all files generated by RSpec
.rspec

# Ignore all files in the public/system directory
/public/system

# Ignore all files in the coverage directory
/coverage/

# Ignore all files in the spec/tmp directory
/spec/tmp

# Ignore all files with the .orig extension
*.orig

# Ignore rerun.txt file
rerun.txt

# Ignore pickle-email-*.html files
pickle-email-*.html

# Ignore all bundler configuration files
/.bundle

# Ignore all SQLite database files and journal files
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-[0-9]*

# Ignore all logfiles and tempfiles
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore the secret token file
config/initializers/secret_token.rb

# Ignore the master key file
config/master.key

# Ignore environment files unless they are templates
.env
.env*.local

# Ignore all files in the .bundle and vendor/bundle directories
/.bundle
/vendor/bundle

# Ignore the .rvmrc file
.rvmrc

# Ignore all files in the vendor/assets/bower_components directory
/vendor/assets/bower_components
*.bowerrc
bower.json

# Ignore the .powenv file
.powenv

# Ignore the .byebug_history file
.byebug_history

# Ignore the node_modules directory
/node_modules/

# Ignore precompiled JavaScript packs
/public/packs
/public/packs-test
/public/assets

# Ignore yarn files
/yarn-error.log
/yarn-debug.log
*.yarn-integrity

# Ignore uploaded files in development
/storage/*
!/storage/.keep
/public/uploads

/app/assets/builds/*
!/app/assets/builds/.keep
